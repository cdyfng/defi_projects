<% if(filenameMap) { %>
    <script src="<%= PP %>/js/<%= filenameMap['vendor.js'] %>"></script>
    <script src="<%= PP %>/js/<%= filenameMap['app.js'] %>"></script>
    <script src="<%= PP %>/js/<%= filenameMap['data.js'] %>"></script>
    <script src="<%= PP %>/js/<%= filenameMap['ethers_helper.js'] %>"></script>
    <% if(locals.scriptname) { %>
        <script src="<%= PP %>/js/<%= filenameMap[locals.scriptname + ".js"] %>"></script>
    <% } %>
<% } else { %>
    <script src="<%= PP %>/js/vendor.js"></script>
    <script src="<%= PP %>/js/app.js"></script>
    <script src="<%= PP %>/js/data.js"></script>
    <script src="<%= PP %>/js/ethers_helper.js"></script>
    <% if(locals.scriptname) { %>
        <script src="<%= PP %>/js/<%= locals.scriptname %>.js"></script>
    <% } %>
<% } %>
